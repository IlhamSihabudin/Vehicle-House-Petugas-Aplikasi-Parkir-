<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ilham.vehiclehousepetugas">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_petugas"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_petugas_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginPetugas"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".Dashboard"
            android:label="@string/title_activity_dashboard" />
        <activity
            android:name=".EditProfil"
            android:label="Edit Profil" />
        <activity
            android:name=".QrScanMasuk"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />

        <activity
            android:name=".DetailMasuk"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />

        <activity android:name=".QrScanKeluar"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>

        <activity android:name=".DetailKeluar"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>


        <meta-data android:name="com.google.android.gms.vision.DEPENDENCIES"
            android:value="barcode" />
    </application>

</manifest>